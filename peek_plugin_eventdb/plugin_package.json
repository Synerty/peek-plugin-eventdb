{
    "plugin": {
        "title": "Event DB",
        "packageName": "peek_plugin_eventdb",
    "version":"0.0.0",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "client",
        "server",
        "storage",
        "admin",
        "agent",
        "worker",
        "doc-dev",
        "doc-admin",
        "client",
        "mobile",
        "desktop",
        "doc-user"
    ],
    "doc-dev": {
        "docDir": "doc-dev",
        "docRst": "index.rst",
        "hasApi": true
    },
    "doc-admin": {
        "docDir": "doc-admin",
        "docRst": "index.rst"
    },
    "doc-user": {
        "docDir": "doc-user",
        "docRst": "index.rst"
    },
    "server": {
    },
    "client": {
    },
    "storage": {
        "alembicDir": "_private/alembic"
    },
    "admin": {
        "showHomeLink": true,
        "appDir": "_private/admin-app",
        "appModule": "eventdb.module#EventDBModule"
    },
    "agent": {
    },
    "worker": {
    },
    "mobile": {
        "titleBarText": "Alarms / Events",
        "showHomeLink": true,
        "appDir": "_private/both-app",
        "appModule": "eventdb.module#EventDBModule",
        "assetDir": "_private/both-assets",
        "icon": "/assets/peek_plugin_object_popup/icon.png",
        "moduleDir": "plugin-module",
        "rootServices": [
            {
                "file": "_private/EventDBTupleService",
                "class": "EventDBTupleService"
            },
            {
                "file": "EventDBService",
                "class": "EventDBService"
            },
            {
                "file": "_private/PrivateEventDBService",
                "class": "PrivateEventDBService",
                "persistent": true
            }
        ]
    },
    "desktop": {
        "titleBarText": "Alarms / Events",
        "showHomeLink": true,
        "appDir": "_private/both-app",
        "appModule": "eventdb.module#EventDBModule",
        "assetDir": "_private/both-assets",
        "icon": "/assets/peek_plugin_object_popup/icon.png",
        "moduleDir": "plugin-module",
        "rootServices": [
            {
                "file": "_private/EventDBTupleService",
                "class": "EventDBTupleService"
            },
            {
                "file": "EventDBService",
                "class": "EventDBService",
                "useExistingClass": "PrivateEventDBService"
            },
            {
                "file": "_private/PrivateEventDBService",
                "class": "PrivateEventDBService",
                "persistent": true
            }
        ]
    }
}
